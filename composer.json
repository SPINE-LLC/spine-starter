{
  "name": "spine/starter",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "Custom starter theme based on Sage 11",
  "homepage": "https://github.com/SPINE-LLC",
  "authors": [
    {
      "name": "Spine LLC",
      "email": "sdev@alignideas.com",
      "homepage": "https://spinellc.com/"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://spinellc.com/"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "roots/acorn": "^5.0"
  },
  "require-dev": {
    "laravel/pint": "^1.20"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
